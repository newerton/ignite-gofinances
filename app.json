{
  "expo": {
    "name": "gofinance",
    "slug": "gofinance",
    "scheme": "gofinance",
    "version": "1.0.0",
    "assetBundlePatterns": ["**/*"],
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5636D3"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.gofinance",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#5636D3"
      }
    }
  }
}
